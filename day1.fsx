(* Split list into lines when too many characters *)

let final = String.concat "" all

let mutable count = 0

final
    |> Seq.iter (fun x ->
            match x with
            | '(' -> count <- count + 1
            | ')' -> count <- count - 1
            | _ -> failwith "Exception" )
            
printfn "Count: %A" count

(* Part II *)

let isBasement count =
    count = "-1"
    
let mutable results = ""

final
    |> Seq.iter (fun x ->
            match x with
            | '(' -> count <- count + 1; results <- results + string (count) + ";"
            | ')' -> count <- count - 1; results <- results + string (count) + ";"
            | _ -> failwith "Exception" )
        
let res = 
    results.Split ';'
    |> Seq.findIndex (fun x -> isBasement x )
    
let basement = res + 1